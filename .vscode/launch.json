{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Pytest: Dev Mark",
			"type": "debugpy",
			"request": "launch",
			"cwd": "${workspaceFolder}/gsp",
			"python": "${command:python.interpreterPath}",
			"module": "pytest",
			"console": "internalConsole",
			"justMyCode": true,
			"args": ["--disable-warnings", "-s", "-v", "-m", "dev", "./tests"]
		},
		{
			"name": "Python: GSP Pipeline",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/gsp/gsp/run.py"
		},
		{
			"name": "Python: MongoDB",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/gsp/gsp/mongodb/run.py"
		},
		{
			"name": "Python: Scraper",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/gsp/gsp/scraper/run.py"
		},
		{
			"name": "Python Debugger: Current File",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		}
	]
}
